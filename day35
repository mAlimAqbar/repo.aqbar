package day35;

public class Main {
    
    public static void main(String[] args) {
        
        int nilai = 100;
        System.out.println("perulangan while aqbar :");
        while (nilai <=400) {
            System.out.println(nilai);
            nilai++;
        }  
        System.out.println("perulangan do while punya aqbar :isc");
        do{
            System.out.println(nilai);
            nilai--;
        } while (nilai <= 5);
        }
        }
